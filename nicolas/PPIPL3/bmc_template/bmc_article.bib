  nlm-unique-id      = {101563288},
  owner              = {NLM},
  publication-status = {ppublish},
  registry-number    = {0 (Proteome)},
  revised            = {20190108},
  source             = {Sci Rep. 2013;3:1613. doi: 10.1038/srep01613.},
