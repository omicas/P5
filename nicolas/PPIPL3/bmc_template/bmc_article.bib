  article-pii            = {3075},
  comment                = {BMC Genomics. 2016 Dec 2;17 (1):988. PMID: 27912746},
  completed              = {20180115},
  electronic-publication = {20160917},
  grantno                = {R01 MH101814/MH/NIMH NIH HHS/United States},
  history                = {2016/09/19 06:01 [medline]},
  issue                  = {1},
